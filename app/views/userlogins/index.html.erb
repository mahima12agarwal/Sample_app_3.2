<h1>Listing userlogins</h1>

<table>
  <tr>
    <th>Username</th>
    <th>Password</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @userlogins.each do |userlogin| %>
  <tr>
    <td><%= userlogin.userName %></td>
    <td><%= userlogin.password %></td>
    <td><%= link_to 'Show', userlogin %></td>
    <td><%= link_to 'Edit', edit_userlogin_path(userlogin) %></td>
    <td><%= link_to 'Destroy', userlogin, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Userlogin', new_userlogin_path %>
